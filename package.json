    "license": "MIT",
    "main": "src/index.ts",
    "scripts": {
        "build": "yarn clean && tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
        "clean": "rimraf dist",
        "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
        "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register --watch src,.env src/index.ts",
        "dev:worker": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register --watch src,.env src/index-worker.ts",
        "dev:ts": "ts-node -r tsconfig-paths/register src/index.ts",
        "start": "node dist/index.js",
        "start:worker": "node --trace-deprecation dist/index-worker.js",
        "prisma:gen": "npx prisma generate",
        "prisma:push": "npx prisma db push",
        "prisma:mig": "npx prisma migrate dev",
        "prisma:seed": "ts-node src/common/seed.ts",
        "prisma:reset": "npx prisma migrate reset --skip-seed && npx prisma db push",
        "prisma:format": "npx prisma format",
        "prisma:studio": "npx prisma studio",
        "clear:uploads": "rimraf uploads && mkdir uploads",
        "pm2:start": "pm2 start ecosystem.config.js",
        "pm2:reload": "pm2 reload ecosystem.config.js",
        "pm2:stop": "pm2 stop ecosystem.config.js",
        "pm2:delete": "pm2 delete ecosystem.config.js",
        "pm2:logs": "pm2 logs"
    },
    "resolutions": {
        "@types/express-serve-static-core": "^4.17.31"
    },
    "prisma": {
        "schema": "src/common/schemas",
        "seed": "ts-node src/common/seed.ts"
    },
    "dependencies": {
        "@datalust/winston-seq": "^2.0.0",
        "@prisma/client": "^6.5.0",
        "@socket.io/redis-emitter": "^5.1.0",
        "bcryptjs": "^2.4.3",
        "bluebird": "^3.7.2",
        "body-parser": "^1.20.3",
        "bullmq": "^5.56.5",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "dotenv": "^16.3.1",
        "dotenv-safe": "^9.1.0",
        "exceljs": "^4.4.0",
        "express": "^4.21.2",
        "express-rate-limit": "^7.5.0",
        "express-session": "^1.18.1",
        "express-validation": "^4.1.1",
        "handlebars": "^4.7.8",
        "helmet": "^8.0.0",
        "i18next": "^24.2.3",
        "i18next-http-middleware": "^3.7.1",
        "ioredis": "^5.4.2",
        "joi": "^17.13.3",
