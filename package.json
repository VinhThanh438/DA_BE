{
  "name": "da_be",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && tsc && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "dev": "nodemon src/index.ts",
    "dev-worker": "nodemon src/index-worker.ts",
    "start": "node dist/index.js",
    "start-worker": "node dist/index-worker.js",
    "prisma:push": "npx prisma db push",
    "prisma:mig": "npx prisma migrate dev",
    "prisma:seed": "ts-node src/common/infrastructure/database/prisma/seed.ts",
    "prisma:reset": "npx prisma migrate reset --skip-seed && npx prisma db push"
  },
  "prisma": {
      "schema": "src/common/schema/schema.prisma",
      "seed": "ts-node src/common/schema/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.3.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.3",
    "bull": "^4.16.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv-safe": "^9.1.0",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validation": "^4.1.1",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/bluebird": "^3.5.42",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/dotenv-safe": "^8.1.6",
    "@types/express": "4.17.21",
    "@types/express-serve-static-core": "4.19.6",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.7",
    "prettier": "^3.4.2",
    "prisma": "^6.3.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "tslint": "^6.1.3",
    "typescript": "^5.7.2"
  }
}
