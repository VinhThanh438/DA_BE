        "module-alias": "^2.2.3",
        "moment-timezone": "^0.5.48",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.10.0",
        "puppeteer": "^24.12.1",
        "request-ip": "^3.3.0",
        "socket.io": "^4.8.1",
        "winston": "^3.17.0",
        "zod": "^3.25.53"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/bluebird": "^3.5.42",
